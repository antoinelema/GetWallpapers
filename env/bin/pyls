#!/home/antoine/projets/GetWallpapers/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'python-language-server==0.21.2','console_scripts','pyls'
__requires__ = 'python-language-server==0.21.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('python-language-server==0.21.2', 'console_scripts', 'pyls')()
    )
